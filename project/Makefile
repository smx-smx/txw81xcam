INCLUDES += -I../sdk/include/lib/net/lwip/include
LDFLAGS += -lm -lwifi_wpa3 -llmac_dsleep -llmac -lcore -lsysctrl -lSHA -lxspi -lFLASH -lUSB -laudio -lble -lnetutils -lcommon -lopenssl -losal -latcmd -lllm

include ../../../platforms/TXW81X/OpenBeken.mk
SRC_O = $(patsubst %.c,%.o,$(SRC_C))
SRC_SO += $(patsubst %.S,%.O,$(SRC_S))

#SRC_C_LIST = $(notdir $(SRC_C))
OBJ_LIST = $(SRC_O)
OBJ_LIST += $(SRC_SO)

.PHONY: application
test_target: prerequirement $(SRC_O) $(SRC_SO)

application: test_target
	@$(CC) -o project.elf -L./../libs \
	-u user_main -march=ck803 -mcpu=ck803s -g -Wl,--gc-sections -Wl,-zmax-page-size=1024 \
	-Xlinker -Map=project.map \
	-Wl,--start-group $(OBJ_LIST) \
	$(LDFLAGS) -Wl,--end-group -T./utilities/gcc_csky.ld \
	-Xlinker -Map=project.map

	@$(OP) --strip-unneeded -O ihex project.elf project.hex
	@$(OP) --strip-unneeded -O binary project.elf project.bin

.PHONY: prerequirement
prerequirement:
	@echo ===========================================================
	@echo Build $(APP_BIN_NAME)
	@echo ===========================================================

$(SRC_O): %.o : %.c
	@ echo "build_c $@"
	@$(CC) $(CFLAGS) $(INCLUDES) $(DEFINES) -c $< -o $@
	@$(CC) $(CFLAGS) $(INCLUDES) $(DEFINES) -c $< -MM -MT $@ -MF $(patsubst %.o,%.d,$@)
	
$(SRC_SO): %.O: %.S
	@ echo "build_S $@"
	@$(CC) $(CFLAGS) $(INCLUDES) $(DEFINES) -c $< -o $@
	@$(CC) $(CFLAGS) $(INCLUDES) $(DEFINES) -c $< -MM -MT $@ -MF $(patsubst %.O,%.d,$@)
	
.PHONY: clean
clean:
	rm -f $(SRC_O)
	rm -f $(SRC_SO)